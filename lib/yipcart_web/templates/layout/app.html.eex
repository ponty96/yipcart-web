    <!-- Optional JavaScript -->
		<!-- jQuery first, then Popper.js, then Bootstrap JS -->
		<script src="javascript/jquery-3.3.1.min.js"></script>
		<script src="javascript/popper.min.js"></script>
		<script src="javascript/bootstrap.min.js"></script>

		<script>
    	$(function () {
        $(window).scroll(function () {
          if ($(window).scrollTop() >= 100) {
            $('nav').addClass('scrolled');
          }
          else {
            $('nav').removeClass('scrolled');
          }
        });
    	});
		</script>

		<!-- Navbar Script -->
		<script>
    	$(function () {
        var Accordion = function (el, multiple) {
          this.el = el || {};
          this.multiple = multiple || false;

          // Variables privadas
          var links = this.el.find('.link');
          // Evento
          links.on('click', {el: this.el, multiple: this.multiple}, this.dropdown)
        }

        Accordion.prototype.dropdown = function (e) {
          var $el = e.data.el;
          $this = $(this),
          $next = $this.next();

          $next.slideToggle();
          $this.parent().toggleClass('open');

          if (!e.data.multiple) {
            $el.find('.ds-accordion--content').not($next).slideUp().parent().removeClass('open');
          };
        }

        var accordion = new Accordion($('#accordion'), false);
    	});
		</script>

		<!-- Navbar Mobile -->
		<script>
    	(function ($) {
        $(document).ready(function () {
          var $menuRevealBtn = $('#menu-reveal');
          var $sideNav = $('#sr-sideNav--mobile');
          var $sideNavMask = $('#sr-sideNav--mobile__mask');

          $menuRevealBtn.on('click', function () {
            $sideNav.addClass('visible');
            $sideNavMask.addClass('visible');
          });

          $sideNavMask.on('click', function () {
            $sideNav.removeClass('visible');
            $sideNavMask.removeClass('visible');
          });
        });
    	})(jQuery)
		</script>
